<meta charset="utf-8" />
<meta http-equiv='X-UA-Compatible' content='IE=edge;chrome=1' />
<title><%= html_title(current_article, current_resource) %></title>
<%= feed_tag :atom, "#{blog.options.prefix.to_s}/feed.xml", title: "Atom Feed" %>
<%= stylesheet_link_tag "normalize", "bootstrap", "clean-blog", "syntax", "fontcustom/fontcustom.css", "custom" %>

<!-- Meta stuff -->
<link rel="canonical" href="http://ashfurrow.com/"/>
<meta property="og:site_name" content="<%= data.site.name %>"/>
<meta property="og:title" content="Blog"/>
<meta property="og:url" content="http://ashfurrow.com/"/>
<meta property="og:type" content="website"/>
<meta property="og:description" content="<%= data.site.description %>"/>
<meta property="og:image" content="<%= data.site.image %>"/>
<meta itemprop="name" content="Blog"/>
<meta itemprop="url" content="http://ashfurrow.com/"/>
<meta itemprop="description" content="<%= data.site.description %>"/>
<meta itemprop="thumbnailUrl" content="<%= data.site.image %>"/>
<link rel="image_src" href="<%= data.site.image %>" />
<meta itemprop="image" content="<%= data.site.image %>"/>
<meta name="description" content="<%= data.site.description %>" />

<!-- Custom Fonts -->
<link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
<link href='http://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
<link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css'>
